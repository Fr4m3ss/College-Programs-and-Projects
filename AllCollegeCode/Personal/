#include <iostream>
#include <string>

using namespace std;

int main()
{
	string input = "";
	string s1 = "";
	string s2 = "";
	string s3 = "";
	string s4 = "";
	int temp = 0;
	int placeholder = 0;

	cout<<"Input String"<<endl;
	cin>>input;

	for(int i = 0; i<input.length(); i++)
	{
		if(input.at(i)!='*'&&temp==0)
		{
			s1=s1+input.at(i);	
		}
		else
		{
			temp = 1;
			placeholder = i+1;
		}
	}
	temp = 0;
	for(int i = placeholder; i<input.length(); i++)
        {
		cout<<"hi"<<endl;
                if(input.at(i)!='*'&&temp==0)
                {
                        s2=s2+input.at(i);
			cout<<input.at(i)<<endl;
                }
                else
                {
                        temp = 1;
			placeholder = i+1;
                }
		cout<<"bye"<<endl;
        }
	temp = 0;
	for(int i = placeholder; i<input.length(); i++)
        {
                if(input.at(i)!='*'&&temp==0)
                {
                        s3=s3+input.at(i);
                }
                else
                {
                        temp = 1;
                        placeholder = i+1;
                }
        }
	temp = 0;
        for(int i = placeholder; i<input.length(); i++)
        {
                if(input.at(i)!='*'&&temp==0)
                {
                        s4=s4+input.at(i);
                }
                else
                {
                        temp = 1;
                        placeholder = i+1;
                }
        }
	cout<<input<<" "<<s1<<" "<<s2<<" "<<s3<<" "<<s4<<endl;

}
